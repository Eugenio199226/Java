package array.ejemplo;

public class Array {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String[] meses = new String[12];
		
		meses[0]="Ene";
		meses[1]="Feb";
		meses[2]="Mar";
		meses[3]="Abr";
		meses[4]="May";
		meses[5]="Jun";
		meses[6]="Jul";
		meses[7]="Ago";
		meses[8]="Sep";
		meses[9]="OCT";
		meses[10]="NOM";
		meses[11]="DIC";
	    for (int i = 0; i < meses.length; i++) {
	    	System.out.println("mes: "+meses[i]);
		}

	}

}
