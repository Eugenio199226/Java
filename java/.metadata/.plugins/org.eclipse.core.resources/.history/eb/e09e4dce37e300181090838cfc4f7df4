package paq.decisor;

public class Principal {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a,b,c;
		a=3;
		b=2;
		c=12;
		if(a>b && a>c) 
		{
			System.out.println(a);
		}else if(b>c)
		{
			System.out.println(b);
		}
		else 
		{
			System.out.println(c);
		}
	}

}
